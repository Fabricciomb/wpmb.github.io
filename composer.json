{
    "name": "fabricciomb/wpmb.github.io",
    "description": "Created to quickly create a production environment",
    "authors": [
        {
            "name": "Fabricciomb",
            "email": "fabricciobuenomartinelli@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url" : "https://wpackagist.org"
        }
    ],
    "require": {
        "wpackagist-plugin/google-xml-sitemaps-v3-for-qtranslate": "*",
        "wpackagist-plugin/akismet": "*",
        "wpackagist-plugin/contact-form-7": "*",
        "wpackagist-plugin/nav-menu-roles": "*",
        "wpackagist-plugin/chats": "*",
        "wpackagist-plugin/wordpress-seo": "*",
        "wpackagist-plugin/woocommerce-delivery-notes": "*",
        "wpackagist-plugin/woocommerce": "*",
        "wpackagist-plugin/woocommerce-correios": "*",
        "wpackagist-plugin/woocommerce-pagseguro": "*",
        "wpackagist-plugin/woocommerce-mercadopago": "*",
        "wpackagist-plugin/phoneme-order-woocommerce": "*",
        "wpackagist-plugin/woocommerce-role-based-price": "*",
        "wpackagist-plugin/wc-abandoned-cart-failed-payment-recovery": "*",
        "wpackagist-plugin/wc-fields-factory": "*",
        "wpackagist-plugin/maintenance": "*",
        "wpackagist-plugin/admin-menu-editor": "*",
        "wpackagist-plugin/erp": "*",
        "wpackagist-plugin/events-manager": "*",
        "wpackagist-plugin/cloudflare-flexible-ssl": "*",
        "wpackagist-plugin/mailchimp-for-wp": "*",
        "wpackagist-plugin/mailchimp-top-bar": "*",
        "wpackagist-plugin/super-socializer": "*",
        "wpackagist-plugin/user-role-editor": "*",
        "wpackagist-plugin/sidebar-login": "*"
    },
    "extra": {
        "installer-paths": {
            "wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
            "wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"]
        }
    }
}